@{
    Layout = null;
}

<script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>

<button type="submit" class="btn btn-primary" data-toggle="collapse" data-target="#AddNew">Add Project</button>
<div id="AddNew" class="box collapsed">
    <!-- /.box-header -->
    <!-- form start -->
    <form role="form" action="/Company/ProjectManagement/Add_New_Project/" method="post">
        <div class="box-body">

            <div class="form-group">
                <label for="exampleInputFile">Project Name</label>
                <input type="text" class="form-control pull-right" name="name" required>
            </div>
            <div class="form-group">
                <label for="exampleInputFile">Description:</label>
                <textarea rows="5" cols="50" class="form-control pull-right" name="Descriptions" required></textarea>
            </div>
            <div class="form-group">
                <label for="exampleInputFile">Manager:</label>
                <div class="dropdown">
                    <input class="form-control" type="text" id="txt_PMember_Email" name="txt_PMember_Email" data-autocomplete-url="autocomplete" />
                </div>
            </div>   
            <div class="form-group">
                <label for="exampleInputFile">Start Date:</label>
                <input name="startday" data-provide="datepicker">
                To <input name="endday" data-provide="datepicker">
            </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            <button type="submit" class="btn btn-primary">Add</button>
        </div>
    </form>
</div>
