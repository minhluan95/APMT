<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>A Process Management Tools | D.o.T Team</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <script src="~/Scripts/jquery-2.2.3.min.js"></script>   

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Content/css/font-awesome.min.css" rel="stylesheet" />
    @*<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">*@
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- jvectormap -->
    <!-- Theme style -->
    
    <link href="~/Content/Titatoogle/dist/titatoggle-dist.css" rel="stylesheet" />

    <link href="~/Content/css/_all-skins.css" rel="stylesheet" />

    <link href="~/Content/css/AdminLTE.css" rel="stylesheet" />

    <link href="~/Content/css/dataTables.bootstrap.css" rel="stylesheet" />

</head>
<body class="body-content hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        @{Html.RenderPartial("~/Areas/Company/Views/Shared/Partial/_Client_HeaderPartial.cshtml"); }
        <!-- Left side column. contains the logo and sidebar -->
        @{Html.RenderPartial("~/Areas/Company/Views/Shared/Partial/_Client_MenuPartial.cshtml"); }

        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> 1.0
            </div>
            <strong>Copyright &copy; 2016-2017 <a style="color:#990000" href="#">D.o.T Team</a>.</strong>
        </footer>
        <!-- Control Sidebar -->
        @{Html.RenderPartial("~/Areas/Company/Views/Shared/Partial/_Client_ConfigPartial.cshtml"); }
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
         immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 2.2.3 -->
    <!-- Bootstrap 3.3.6 -->

    <script src="~/Scripts/bootstrap.min.js"></script>
    
    <script src="~/Content/Boostrap Confirmation/bootstrap-confirmation.js"></script>

    <script src="~/Scripts/app.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.min.js"></script>

    <script type="text/javascript">
        $(function () {
            $('#datetimepicker').datepicker();
        });
    </script>

    <script src="~/Content/css/jquery.dataTables.js"></script>

    <script src="~/Content/css/dataTables.bootstrap.min.js"></script>
    <!--Confirm Setrole Form-->
    <div class="modal fade" id="confirm-SetRole" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    Set Role confirm
                </div>
                <div class="modal-body">
                    Are you sure ?
                </div>
                <div class="modal-footer">
                    <a class="btn btn-danger btn-ok">Yes</a>
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                </div>
            </div>
        </div>
    </div>
    <!--Confirm Delete Form-->
    <div class="modal fade" id="confirm-delete" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    Delete confirmation
                </div>
                <div class="modal-body">
                    This is very dangerous, you shouldn't do it! Are you really really sure?
                </div>
                <div class="modal-footer">
                    <a class="btn btn-danger btn-ok" id="btnOK">Yes</a>
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                </div>
            </div>
        </div>
    </div>
    <!--Confirm Disable Form-->
    <div class="modal fade" id="confirm-disable" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    Disable confirm
                </div>
                <div class="modal-body">
                    This is very dangerous, you shouldn't do it! Are you really really sure?
                </div>
                <div class="modal-footer">
                    <a class="btn btn-danger btn-ok">Yes</a>
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                </div>
            </div>
        </div>
    </div>
    <!--Confirm Set Creator Form -->
    <div class="modal fade" id="confirm-creator" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    Set Role confirm
                </div>
                <div class="modal-body">
                    This is very dangerous, you shouldn't do it! Are you really really sure?
                </div>
                <div class="modal-footer">
                    <a class="btn btn-danger btn-ok">Yes</a>
                    <button type="button" class="btn btn-default" data-dismiss="modal">No</button>
                </div>
            </div>
        </div>
    </div>
<style>
    .modal-body.large{
        width: 80%;
        height:80%;
    }
</style>
    

    <script type="text/javascript">
        $('#confirm-delete').on('show.bs.modal', function (e) {
            $(this).find('.btn-ok').attr('href', $(e.relatedTarget).data('href'));
        });
        $('#confirm-disable').on('show.bs.modal', function (e) {
            $(this).find('.btn-ok').attr('href', $(e.relatedTarget).data('href'));
        });
        $('#confirm-creator').on('show.bs.modal', function (e) {
            $(this).find('.btn-ok').attr('href', $(e.relatedTarget).data('href'));
        });
    </script>

    <script type="text/javascript" language="javascript">

        $(document).ready(function (request) {
            $("#somevalue").autocomplete({
                source: 'autocomplete',
                //data: "{ 'term':'" + request.term + "'}",
                data: "{term : '"+request.term+"'}",

            });
        })
    </script>
    <!-- Modal -->
    

    <!-- Sparkline -->
    <!-- ChartJS 1.0.1 -->
    @* <script src="~/Scripts/dashboard2.js"></script>*@
    
</body>
</html>